<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - VDOEasy</title>
    @* <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" /> *@
    <link rel="stylesheet" href="~/lib/mazer/css/mazer.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/mazer/css/mazer-dark.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/mazer/css/iconly.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/VDOEasy.styles.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/DataTables/datatables.min.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/lib/sweetalert/sweetalert2.min.css" asp-append-version="true" />
</head>
<body>
    <div id="app">
        <vc:left-navigation></vc:left-navigation>
        <div id="main">
            <main role="main" class="pb-3">
                <header class="mb-3">
                    <a href="#" class="burger-btn d-block d-xl-none">
                        <i class="bi bi-justify fs-3"></i>
                    </a>
                </header>
                <vc:top-navigation></vc:top-navigation>
                @RenderBody()
            </main>
        </div>
    </div>
    

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/lib/mazer/js/mazer.js"></script>
    <script src="~/lib/mazer/js/dark.js"></script>
    <script src="~/lib/DataTables/datatables.min.js"></script>
    <script src="~/lib/sweetalert/sweetalert2.min.js"></script>
    @* <script src="~/lib/mazer/js/sidebar.js"></script> *@
    @* <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script> *@
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
